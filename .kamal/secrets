SECRETS=$(kamal secrets fetch --adapter 1password --account instaleap-llc.1password.com --from SERVER/TAMAYOTCHI SECRET_KEY_BASE KAMAL_REGISTRY_PASSWORD TUNNEL_TOKEN)
KAMAL_REGISTRY_PASSWORD=$(kamal secrets extract KAMAL_REGISTRY_PASSWORD $SECRETS)
SECRET_KEY_BASE=$(kamal secrets extract SECRET_KEY_BASE $SECRETS)
TUNNEL_TOKEN=$(kamal secrets extract TUNNEL_TOKEN $SECRETS)
